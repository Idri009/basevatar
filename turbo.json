{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
        },
        "dev": {
            "dependsOn": ["^db:generate"],
            "cache": false,
            "persistent": true
        },
        "start": {
            "dependsOn": ["build"],
            "cache": false,
            "persistent": true
        },
        "db:generate": {
            "dependsOn": ["^db:generate"],
            "cache": false
        },
        "db:seed": {
            "dependsOn": ["^db:seed"],
            "cache": false
        },
        "typecheck": {
            "dependsOn": ["db:generate", "^db:generate"]
        },
        "lint": {
            "dependsOn": []
        },
        "clean": {
            "cache": false
        }
    }
}
